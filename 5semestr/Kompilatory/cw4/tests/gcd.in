# gcd, recursive
int gcd(int m, int n) {

int res = 0;
if (m!=n) { 
    if (m > n)
        res = gcd(m-n, n);
    else
        res = gcd(n-m, m);
}
else
    res = m;

return res;
}


# gcd, iterative
int gcd2(int a, int b) {
    int c = 0;

    while( a != 0 ) {
        c = a;
        a = b % a;
        b = c;
    }

    return b;
}


int lcm(int a, int b) {
    return (a*b) / gcd(a,b);
}


print gcd(125,100);
print gcd2(125,100);
print lcm(125,100);
