
int a = 0, b = 0, c = 0;

# factorial, recursive
int fact(int n) {
  if(n==0) return 1;
  else if(n==1) return 1;
  else return n*fact(n-1);
}


# factorial, iterative
int fact2(int x) {
    int c = 1, res = 1;
    
    repeat
        res = res * c;
        c = c + 1;
    until c > x;
    return res;
}

print fact(0);
print fact(1);
print fact(10);
print fact2(10);
print fact(20);
print fact2(20);
